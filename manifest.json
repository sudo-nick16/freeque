{
  "name": "FreeQue",
  "description": "Quizlet free",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://quizlet.com/*"],
      "js": ["./magic.js"]
    }
  ],
  "content_security_policy": {
    "script-src": "self",
    "object-src": "none"
    }
}
